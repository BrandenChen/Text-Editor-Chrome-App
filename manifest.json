{
  "name": "The Word Editor Extension",
  "version": "0.0.2",
  "description": "Edit words on webpages",
  "manifest_version": 3,
  "background":{
    "service_worker": "serviceWorker.js"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["scripting", "storage"],
  "options_ui":{
    "page": "option.html",
    "open_in_tab" : true
  }
}
